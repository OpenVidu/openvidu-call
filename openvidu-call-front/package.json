    "name": "openvidu-call",
    "private": true,
    "scripts": {
        "build": "./node_modules/@angular/cli/bin/ng build && npm run copy:backend",
        "build-prod": "func() { ./node_modules/@angular/cli/bin/ng build --configuration production --base-href=\"${1:-/}\" && npm run copy:backend-prod; }; func",
        "build-win:openvidu-angular": "node .\\openvidu-angular-build.js && node .\\node_modules\\@angular\\cli\\bin\\ng build openvidu-angular --configuration production",
        "build-win:openvidu-webcomponent": "node .\\openvidu-webcomponent-prepare.js && node .\\node_modules\\@angular\\cli\\bin\\ng build --configuration production --output-hashing none && node .\\openvidu-webcomponent-build.js",
        "build:openvidu-angular": "node openvidu-angular-build.js && ./node_modules/@angular/cli/bin/ng build openvidu-angular --configuration production",
        "build:openvidu-webcomponent": "node ./openvidu-webcomponent-prepare.js && ./node_modules/@angular/cli/bin/ng build --configuration production --output-hashing none && node ./openvidu-webcomponent-build.js",
        "build:openvidu-webcomponent-pro": "node ./openvidu-webcomponent-prepare.js pro && ./node_modules/@angular/cli/bin/ng build openvidu-call-pro --configuration production --output-hashing none && node ./openvidu-webcomponent-build.js pro",
        "copy:backend": "cp -a dist/openvidu-call/. ../openvidu-call-back/public/",
        "copy:backend-prod": "mkdir -p ../openvidu-call-back/dist/public && cp -a dist/openvidu-call/. ../openvidu-call-back/dist/public/",
        "e2e": "ng e2e",
        "lint": "ng lint",
        "pack:openvidu-angular": "cd dist/openvidu-angular && npm pack",
        "start": "ng serve",
        "start-pro": "func() { ng serve openvidu-call-pro --configuration=dev --port \"${1:-4200}\"; }; func",
        "start:https": "ng serve --host 192.168.1.161 --ssl --ssl-key cert.key --ssl-cert cert.crt",
        "test": "ng test openvidu-call --watch=false --code-coverage"
    },
    "version": "2.18.0"
}
